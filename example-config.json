{
    "order": ["network_interface", "battery", "memory_usage", "volume", "time"],

    "network_interface": {
        "format": "{is_connected:{per_interface_fmt_str:leaked ip: {ipv4_addrs:1}}}",
        "update_interval": 1
    },

    "battery": {
        "format":
            "{has_battery:{per_battery_fmt_str:{is_full:Battery is Full}{is_charging:bat: charging {capacity}%}{is_discharging:bat: discharging {capacity}%}}}"
    },

    "memory_usage": {
        "format": "mem: {MemUsed:M} / {MemTotal:M}", 
        "update_interval": 1
    },

    "volume": {

    },

    "time": {

    }

}
